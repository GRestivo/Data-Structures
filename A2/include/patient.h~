#ifndef PATIENT_H
#define PATIENT_H

typedef struct patient {
  int priority;
  int processTime;
  int entryPlace;
  char *clientID;
  char *symptom;
} Patient;

Patient *createPatient(char *clientID, int priority, char *symptom, int processTime, int entryPlace);

void setTimeFinished(int timeFinished, Patient *p);
#endif
